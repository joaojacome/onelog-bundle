{
    "name": "koderhut/onelog-bundle",
    "description": "A simplified way to inject loggers into objects for Symfony apps",
    "type": "symfony-bundle",
    "authors": [
        {
            "name": "Denis-Florin Rendler",
            "email": "connect@rendler.me"
        }
    ],
    "autoload": {
        "psr-4": { "KoderHut\\OnelogBundle\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "^7.3@dev",
        "phpstan/phpstan": "^0.10.0@dev",
        "symfony/monolog-bridge": "~2.7|~3.3|~4.0",
        "symfony/dependency-injection": "~2.7|~3.4.10|^4.0.10",
        "symfony/config": "~2.7|~3.3|~4.0",
        "symfony/http-kernel": "~2.7|~3.3|~4.0",
        "monolog/monolog": "~1.22",
        "nyholm/symfony-bundle-test": "dev-master",
        "satooshi/php-coveralls": "2.0.x-dev"
    },
    "license": "MIT",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "require": {
        "php": ">=7.1"
    }
}
